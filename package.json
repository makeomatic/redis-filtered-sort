    "redis",
    "sort",
    "filter",
    "multi-sort"
  ],
  "author": "Vitaly Aminev <v@makeomatic.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makeomatic/redis-filtered-sort/issues"
  },
  "homepage": "https://github.com/makeomatic/redis-filtered-sort#readme",
  "peerDependencies": {
    "ioredis": "2.x.x || 3.x.x || 4.x.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-transform-strict-mode": "^7.16.7",
    "@babel/register": "^7.16.7",
    "@makeomatic/deploy": "^11.0.2",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "eslint-config-makeomatic": "^5.1.0",
    "faker": "^5.5.3",
    "ioredis": "^4.28.2",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "cluster-key-slot": "^1.1.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.snakecase": "^4.1.1"
